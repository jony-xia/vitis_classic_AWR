<div class="line"><a id="l02793" name="l02793"></a><span class="lineno"> 2793</span>                        retVal1 += rlAppendSubBlock(&amp;rl_txMsg[devIndex].payload[payloadLen],</div>
<div class="line"><a id="l02794" name="l02794"></a><span class="lineno"> 2794</span>                                                inMsg.subblocks[indx].sbid,</div>
<div class="line"><a id="l02795" name="l02795"></a><span class="lineno"> 2795</span>                                                inMsg.subblocks[indx].len,</div>
<div class="line"><a id="l02796" name="l02796"></a><span class="lineno"> 2796</span>                                                inMsg.subblocks[indx].pSblkData);</div>
<div class="line"><a id="l02797" name="l02797"></a><span class="lineno"> 2797</span>                        <span class="comment">/* increment payload length as appending each sub-block data in a</span></div>
<div class="line"><a id="l02798" name="l02798"></a><span class="lineno"> 2798</span><span class="comment">                           message */</span></div>
<div class="line"><a id="l02799" name="l02799"></a><span class="lineno"> 2799</span>                        payloadLen += inMsg.subblocks[indx].len + \</div>
<div class="line"><a id="l02800" name="l02800"></a><span class="lineno"> 2800</span>                                                    (rlUInt16_t)(RL_SBC_ID_SIZE + RL_SBC_LEN_SIZE);</div>
<div class="line"><a id="l02801" name="l02801"></a><span class="lineno"> 2801</span>                        <span class="keywordflow">if</span> (RL_RET_CODE_OK != retVal1)</div>
